    482          static void _itoa(uint16 num, uint8 *buf, uint8 radix)
    483          {
    484            char c,i;
    485            uint8 *p, rst[5];
    486           
    487            p = rst;
    488            for ( i=0; i<5; i++,p++ )
    489            {
    490              c = num % radix;  // Isolate a digit
    491              *p = c + (( c < 10 ) ? '0' : '7');  // Convert to Ascii
    492              num /= radix;
    493              if ( !num )
    494                break;
    495            }
    496           
    497            for ( c=0 ; c<=i; c++ )
    498              *buf++ = *p--;  // Reverse character order
    499           
    500            *buf = '\0';
    501          }

