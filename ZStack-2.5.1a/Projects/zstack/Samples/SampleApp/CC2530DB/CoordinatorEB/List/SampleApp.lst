    117          /**
    118            终端设备P1端口中断处理函数，用来接收投币器的信号
    119          */
    120          #ifdef END_DEVICE
    121          uint8 pre_coin_pin_value = 1;
    122          uint8 coin_pin_value = 1;
    123          uint8 pre_reward_pin_value = 1;
    124          uint8 reward_pin_value = 1;
    125          HAL_ISR_FUNCTION( port0Isr, P0INT_VECTOR ) // P0_5配置为投币器电平变化中断引脚，在HAL/Common/hal_drivers.c中进行的引脚初始化
    126          { 
    127            IEN1 &= ~(0x1 << 5); // 关闭端口0中断
    128            if((P0IFG >> 4) & 0x1 == 1) { // 中断p0_4引脚产生中断，投币信号    
    129              coin_pin_value = COIN_RECV_PIN;
    130              if(coin_pin_value == 0) {
    131                if(coin_pin_value != pre_coin_pin_value) {
    132                  pre_coin_pin_value = coin_pin_value;
    133                  NeedCoinTransport = 1;
    134                  osal_start_timerEx( SampleApp_TaskID, SAMPLEAPP_TRANSPORT_COIN_MSG_EVT, COIN_SIGNAL_HALF_TIME); // 产生一个定时器事件    
    135                }
    136              } else {
    137                pre_coin_pin_value = 1;
    138              }
    139              P0IFG &= ~(0x1 << 4);       //端口0中断状态标志    
    140              //myprintf("get p0_4 coin input interrupt pin = %d\n", COIN_RECV_PIN);
    141            } 
    142            if((P0IFG >> 6) & 0x1 == 1) { // P0_6引脚，彩票机出一个彩票中断信号，转发到游戏机并上传服务器
    143              startRewardInterrupt = 1;
    144              lastRecvRewardSecTime = osal_getClock(); // 设置收到信号当前的秒数，在定时器中进行检测
    145              reward_pin_value = REWARD_OUTPUT_PIN;
    146              if(reward_pin_value == 0) {
    147                if(reward_pin_value != pre_reward_pin_value) {
    148                  pre_reward_pin_value = reward_pin_value;
    149                  rewardCount += 1;
    150                }      
    151              } else {
    152                pre_reward_pin_value = 1;
    153              }
    154              P0IFG &= ~(0x1 << 6);
    155              myprintf("\nP0_6=%d", REWARD_OUTPUT_PIN); // 此条串口打印语句不能去掉，否则计数会产生不准确结果
    156            }
    157            P0IF = 0;        //端口0中断标志，0表示无中断未决，1表示有中断未决，需要每次触发之后置0，不然会一直触发中断 
    158            IEN1 |= (0x1 << 5); // 端口0中断使能
    159          }
