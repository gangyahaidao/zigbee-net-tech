    130          /**
    131            终端设备P1端口中断处理函数，用来接收投币器的信号
    132          */
    133          #ifdef END_DEVICE
    134          HAL_ISR_FUNCTION( port0Isr, P0INT_VECTOR ) // P0_5配置为投币器电平变化中断引脚，在HAL/Common/hal_drivers.c中进行的引脚初始化
    135          { 
    136            if((P0IFG >> 4) & 0x1 == 1) { // 中断p0_4引脚产生中断，投币信号
    137              P0IFG &= ~(0x1 << 4);       //端口0中断状态标志
    138              NeedCoinTransport = 1;
    139              osal_start_timerEx( SampleApp_TaskID, SAMPLEAPP_TRANSPORT_COIN_MSG_EVT, COIN_SIGNAL_HALF_TIME); // 产生一个定时器事件
    140              myprintf("get p0_4 coin input interrupt\n");
    141            } 
    142            if((P0IFG >> 6) & 0x1 == 1) { // P0_6引脚，彩票机出一个彩票中断信号，转发到游戏机并上传服务器
    143              P0IFG &= ~(0x1 << 6);    
    144              osal_start_timerEx( SampleApp_TaskID, SAMPLEAPP_TRANSPORT_REWARD_MSG_EVT, REWARD_SIGNAL_HALF_TIME); // 产生一个定时器事件
    145              myprintf("Get p0_6 one reward interrupt\n");
    146            }
    147            
    148            P0IF = 0;        //端口0中断标志，0表示无中断未决，1表示有中断未决，需要每次触发之后置0，不然会一直触发中断 
    149          }
    150          #endif
