    127          /**
    128            终端设备P1端口中断处理函数，用来接收投币器的信号
    129          */
    130          #ifdef END_DEVICE
    131          HAL_ISR_FUNCTION( port0Isr, P0INT_VECTOR ) // P0_5配置为投币器电平变化中断引脚，在HAL/Common/hal_drivers.c中进行的引脚初始化
    132          { 
    133            //IEN1 &= ~(0x1 << 5); // 关闭端口0中断
    134            if((P0IFG >> 4) & 0x1 == 1) { // 中断p0_4引脚产生中断，投币信号
    135              P0IFG &= ~(0x1 << 4);       //端口0中断状态标志
    136              NeedCoinTransport = 1;
    137              osal_start_timerEx( SampleApp_TaskID, SAMPLEAPP_TRANSPORT_COIN_MSG_EVT, COIN_SIGNAL_HALF_TIME); // 产生一个定时器事件
    138              myprintf("get p0_4 coin input interrupt\n");
    139            } 
    140            if((P0IFG >> 6) & 0x1 == 1) { // P0_6引脚，彩票机出一个彩票中断信号，转发到游戏机并上传服务器
    141              P0IFG &= ~(0x1 << 6);
    142              startRewardInterrupt = 1;
    143              rewardCount++;    
    144              lastRecvRewardSecTime = osal_getClock(); // 设置收到信号当前的秒数，在定时器中进行检测
    145              myprintf("Get p0_6 one reward interrupt\n");
    146            }
    147            
    148            P0IF = 0;        //端口0中断标志，0表示无中断未决，1表示有中断未决，需要每次触发之后置0，不然会一直触发中断 
    149            //IEN1 |= (0x1 << 5); // 端口0中断使能
    150          }
    151          #endif
